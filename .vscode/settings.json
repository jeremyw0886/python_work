{
  "cSpell.words": [
    "interdimensional"
  ],
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.rulers": [79],
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "flake8.args": ["--max-line-length=79"],
  "python.formatting.provider": "black",
  "isort.args": ["--profile", "black", "--line-length", "79"],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "python.testing.pytestArgs": [
    "."
  ],
  "editor.defaultFormatter": "GitHub.copilot",
  "editor.defaultFoldingRangeProvider": "ms-python.black-formatter",
  "editor.rulers": [79],
  "editor.formatOnSave": true,
"editor.indentSize": "tabSize",
"notebook.defaultFormatter": "GitHub.copilot",
}
